<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <ProxyX64 Include="$(MSBuildThisFileDirectory)..\contentFiles\libraries\x64\proxy.dll" />
    <ProxyX86 Include="$(MSBuildThisFileDirectory)..\contentFiles\libraries\x86\proxy.dll" />
    <None Include="@(ProxyX64)">
      <Link>x64\proxy.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="@(ProxyX86)">
      <Link>x86\proxy.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>